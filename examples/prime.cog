Def Factor (Zero? Modulo Swap);

Def Primes (
	Let U is upper bound;
	initially List ();
	For Range 2 to U (
		Let P is potential prime;
		Let Primes is found primes;
		Let To-check be Take-while (<= Sqrt P) Primes;
		When All (Not Factor of P) To-check (
			Append P
		) to Primes
	)
);

Print Primes up to 1000000000;
